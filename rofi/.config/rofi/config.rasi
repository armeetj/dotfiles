configuration {
    modi: "combi,drun,run,window,ssh";
    combi-modi: "drun,run,window";
    font: "Iosevka Nerd Font 14";
    icon-theme: "Oranchelo";
    show-icons: true;
    terminal: "kitty";
    drun-display-format: "{icon} {name}";
    location: 0;
    disable-history: false;
    hide-scrollbar: true;
    sidebar-mode: true;
}

@theme "base16-mountain"
